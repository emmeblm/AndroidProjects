<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lemme.holamundo" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SegundoActivity"
            android:label="@string/title_activity_segundo" >
        </activity>
        <activity
            android:name=".TercerActivity"
            android:label="@string/title_activity_tercer" >
        </activity>
        <activity
            android:name=".CuartoActivity"
            android:label="@string/title_activity_cuarto" >
        </activity>
    </application>

</manifest>
